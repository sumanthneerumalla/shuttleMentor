{
	"name": "shuttlementor",
	"version": "0.1.0",
	"private": true,
	"type": "module",
	"scripts": {
		"docker:dev": "docker compose -f docker-compose.yml -f docker-compose.dev.yml up --build",
		"docker:prod": "export $(grep -v '^#' .env.production | xargs) && docker compose -f docker-compose.yml -f docker-compose.prod.yml up --build",
		"docker:prod:clean": "docker compose -f docker-compose.yml -f docker-compose.prod.yml down && export $(grep -v '^#' .env.production | xargs) && docker compose -f docker-compose.yml -f docker-compose.prod.yml up --build",
		"docker:build": "docker compose build",
		"docker:start": "docker compose up -d",
		"docker:dev:stop": "docker compose -f docker-compose.yml -f docker-compose.dev.yml down",
		"docker:prod:stop": "docker compose -f docker-compose.yml -f docker-compose.prod.yml down",
		"docker:db:migrate:dev": "docker compose -f docker-compose.yml -f docker-compose.dev.yml exec app npm run db:migrate",
		"docker:db:migrate:prod": "docker compose -f docker-compose.yml -f docker-compose.prod.yml exec app npm run db:migrate",
		"docker:db:studio:dev": "docker compose -f docker-compose.yml -f docker-compose.dev.yml exec app npx prisma studio --port 5555",
		"docker:db:studio:prod": "docker compose -f docker-compose.yml -f docker-compose.prod.yml exec app npx prisma studio --port 5556",
		"docker:debug": "./docker-debug.sh",
		"build": "next build",
		"check": "biome check .",
		"check:unsafe": "biome check --write --unsafe .",
		"check:write": "biome check --write .",
		"db:generate": "prisma migrate dev",
		"db:migrate": "prisma migrate deploy",
		"db:push": "prisma db push",
		"db:studio": "prisma studio",
		"dev": "next dev --turbo",
		"postinstall": "prisma generate",
		"preview": "next build && next start",
		"start": "next start",
		"test": "vitest --run",
		"test:watch": "vitest",
		"test:ui": "vitest --ui",
		"typecheck": "tsc --noEmit"
	},
	"dependencies": {
		"@clerk/nextjs": "^6.35.6",
		"@prisma/client": "6.19.0",
		"@prisma/studio-core": "^0.14.0",
		"@radix-ui/react-avatar": "^1.1.10",
		"@radix-ui/react-slot": "^1.2.3",
		"@t3-oss/env-nextjs": "^0.12.0",
		"@tanstack/react-query": "^5.69.0",
		"@trpc/client": "^11.0.0",
		"@trpc/react-query": "^11.0.0",
		"@trpc/server": "^11.0.0",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"lucide-react": "^0.541.0",
		"next": "^15.2.3",
		"react": "^19.0.0",
		"react-dom": "^19.0.0",
		"react-image-crop": "^11.0.10",
		"server-only": "^0.0.1",
		"superjson": "^2.2.1",
		"tailwind-merge": "^3.3.1",
		"zod": "^3.24.2"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@flydotio/dockerfile": "^0.7.10",
		"@shadcn/ui": "^0.0.4",
		"@tailwindcss/postcss": "^4.0.15",
		"@testing-library/jest-dom": "^6.9.1",
		"@testing-library/react": "^16.3.0",
		"@testing-library/user-event": "^14.6.1",
		"@types/node": "^20.14.10",
		"@types/react": "^19.0.0",
		"@types/react-dom": "^19.0.0",
		"@vitejs/plugin-react": "^5.1.2",
		"fast-check": "^4.4.0",
		"jsdom": "^27.3.0",
		"postcss": "^8.5.3",
		"prisma": "6.19.0",
		"tailwindcss": "^4.0.15",
		"tailwindcss-animate": "^1.0.7",
		"typescript": "^5.8.2",
		"vitest": "^4.0.15"
	},
	"ct3aMetadata": {
		"initVersion": "7.39.3"
	},
	"packageManager": "npm@10.7.0"
}
